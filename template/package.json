  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "*.svg",
      "*.less"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": [
        "standard"
      ]
    }
  },
  "engines": {
    "node": "> 8.1.4",
    "npm": ">= 5.2.0"
  }
}
