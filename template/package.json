{
  "name": "{{name}}",
  "version": "{{version}}",
  "description": "{{description}}",
  "author": "{{author}}",
  "license": "MIT",
  "scripts": {
    "prepush": "npm run ilint -q",
    "dll": "npx rimraf dist && npx cross-env NODE_ENV=development npx webpack --config webpack.dll.config.js",
    "postinstall": "npm -s run dll",
    "dev": "npx cross-env NODE_ENV=development node ./build/dev-server.js",
    "build": "npx rimraf dist && npx cross-env NODE_ENV=production npx webpack --config ./build/webpack.prod.config.js --progress --hide-modules",
    "ilint": "npx standard",
    "fix": "npx standard --fix",
    "commitmsg": "commitlint -E GIT_PARAMS"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "history": "^4.7.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-router-dom": "^4.3.1",
    {{#if_eq state 'redux'}}
    "react-redux": "^5.0.7",
    "connected-react-router": "^4.3.0",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.0",
    "redux-actions-promise": "^1.1.3",
    {{/if_eq}}
    {{#if_eq state 'mobx'}}
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.3",
    "mobx-react-router": "^4.0.4",
    {{/if_eq}}
    "async-await-error-handling": "^0.5.2",
    "react-loadable": "^5.5.0",
    "axios": "^0.18.0",
    "normalize.css": "^8.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-angular": "^7.0.1",
    "autoprefixer": "^9.1.3",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.15",
    "prop-types": "^15.6.2",
    "react-dev-utils": "^6.0.0-next.3e165448",
    "compression-webpack-plugin": "^1.1.11",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "eslint": "^5.4.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "standard": "^11.0.1",
    "mini-css-extract-plugin": "^0.4.1",
    "my-ip": "^1.1.1",
    "happypack": "^5.0.0",
    "html-webpack-plugin": "^3.2.0",
    "plugin-error": "^1.0.1",
    "chalk": "^2.4.1",
    "husky": "^0.14.3",
    "less": "^3.8.0",
    "less-loader": "^4.1.0",
    "open-browser-webpack-plugin": "^0.0.5",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss-safe-parser": "^4.0.1",
    "postcss-loader": "^2.1.6",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-md5-hash": "^0.0.6",
    "webpack-inline-manifest-plugin": "^4.0.1",
    "progress-bar-webpack-plugin": "^1.11.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack-merge": "^4.1.3"
