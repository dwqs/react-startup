{
  "name": "{{name}}",
  "version": "{{version}}",
  "description": "{{description}}",
  "author": "{{author}}",
  "license": "MIT",
  "scripts": {
    "prepush": "npm run ilint -q",
    "dll": "npx webpack --config webpack.dll.config.js",
    "postinstall": "npm -s run dll",
    "dev": "npx cross-env NODE_ENV=development node ./build/dev-server.js",
    "build": "npx rimraf dist && npm run dll && npx cross-env NODE_ENV=production npx webpack --config ./build/webpack.prod.config.js --progress --hide-modules",
    "ilint": "npx eslint src/**/*.js",
    "fix": "npx eslint --fix src/**/*.js",
    "commitmsg": "npx commitlint -e"
  },
  "dependencies": {
    "history": "^4.7.2",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.2.2",
    {{#if_eq state 'redux'}}
    "react-redux": "^5.0.6",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^3.7.2",
    "redux-actions": "^2.3.0",
    "redux-actions-promise": "^1.1.3",
    {{/if_eq}}
    {{#if_eq state 'mobx'}}
    "mobx": "^4.1.0",
    "mobx-react": "^5.0.0",
    "mobx-react-router": "^4.0.1",
    {{/if_eq}}
    "async-await-error-handling": "^0.5.2",
    "async-react-component": "^0.7.0",
    "axios": "^0.18.0",
    "normalize.css": "^8.0.0",
    "babel-polyfill": "^6.26.0"
  },
